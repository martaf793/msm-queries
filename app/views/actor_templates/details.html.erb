<h1>Actor <%=@the_actor.id%></h1>
<a href="/actors">Go back</a>
<hr>
  <dl>
  <dt>Name</dt>
  <dd> <%=@the_director.name%></dd>
  
  <dt>Dob</dt>
  <dd> <%=@the_director.dob%></dd>

  <dt>Bio</dt>
  <dd> <%=@the_director.bio%></dd>

  <dt>Image</dt>
  <dd> <img scr="<%=@the_director.image%>" width="200"</dd>

  <dt>Created at</dt>
  <dd> <%=@the_director.created_at%></dd>

  <dt>Updated at</dt>
  <dd> <%=@the_director.updated_at%></dd>
  </dl>
</hr>

 <%=matching_characters=Character.where({:actor_id=> @the_actor.id})%>
 <table border="1">
 <tr>
  <th>Title</th>
  <th>Year</th>
  <th>Director</th>
  <th>Character</th>
  <th>Image </th>
 </tr>
 <%=matching_characters.each do |a_character|%>

 <%end%>



</table>
